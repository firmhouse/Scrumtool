!!!
%html
  %head
    %title Team Dashboard
    
    = stylesheet_link_tag "social-buttons/social_buttons.css"
    
    = stylesheet_link_tag 'screen.css', :media => 'screen, projection'
    = stylesheet_link_tag 'print.css', :media => 'print'
    /[if lt IE 8]
      = stylesheet_link_tag 'ie.css', :media => 'screen, projection'
    = javascript_include_tag "jquery", "jquery.colorbox-min", "rails", "application"
    
    = csrf_meta_tag
  %body.two-col
    = render "layouts/shared/app_header"
  
    #container
      = yield
